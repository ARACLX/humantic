{
	"name": "@araclx/humantic",
	"version": "1.0.0",
	"private": true,
	"description": "No-code repository considered as a bare template for TypeScript applications, which includes code-related things such as linters, documentation, and other shit that you setup in every new repository. ",
	"repository": "https://github.com/ARACLX/humantic.git",
	"license": "MIT",
	"author": "Jakub Olan <jakub.olan001@gmail.com>",
	"main": "index.js",
	"scripts": {
		"build": "lerna run build",
		"build:watch": "lerna run build:watch --parallel --no-bail",
		"postinstall": "lerna bootstrap --ignore-prepublish",
		"prestart": "yarn build",
		"start": "pm2 start cluster.yaml && yarn build:watch",
		"test": "lerna run test",
		"test:cov": "lerna run test:cov",
		"test:lint": "lerna run test:lint --stream",
		"test:unit": "lerna run test:unit"
	},
	"devDependencies": {
		"@araclx/husky-config": "0.0.13",
		"@araclx/prettier-config": "0.0.11",
		"@araclx/semantic-release": "0.1.6-alpha.0",
		"@araclx/xo-config": "0.0.1-alpha.0",
		"@types/eslint-plugin-prettier": "3.1.0",
		"@types/prettier": "2.0.2",
		"@types/semantic-release": "17.1.0",
		"@types/xo": "0.28.0",
		"eslint-config-xo-react": "^0.23.0",
		"eslint-plugin-prettier": "3.1.4",
		"eslint-plugin-react": "^7.20.6",
		"eslint-plugin-react-hooks": "^4.1.0",
		"eslint-plugin-simple-import-sort": "^5.0.3",
		"husky": "4.2.5",
		"lerna": "3.22.1",
		"pm2": "^4.4.0",
		"prettier": "2.0.5",
		"prettier-plugin-organize-imports": "^1.1.1",
		"prettier-plugin-packagejson": "^2.2.5",
		"semantic-release": "17.1.1",
		"xo": "0.33.0"
	}
}
